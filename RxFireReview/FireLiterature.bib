@article{mcwethy2019,
  title = {Rethinking Resilience to Wildfire},
  author = {McWethy, David B. and Schoennagel, Tania and Higuera, Philip E. and Krawchuk, Meg and Harvey, Brian J. and Metcalf, Elizabeth C. and Schultz, Courtney and Miller, Carol and Metcalf, Alexander L. and Buma, Brian and Virapongse, Arika and Kulig, Judith C. and Stedman, Richard C. and Ratajczak, Zak and Nelson, Cara R. and Kolden, Crystal},
  date = {2019-09},
  journaltitle = {Nature Sustainability},
  volume = {2},
  pages = {797--804},
