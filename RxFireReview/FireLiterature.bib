@InCollection{sarkar2007,
  author    = {Sarkar, Sahotra},
  booktitle = {The {{Cambridge Companion}} to the {{Philosophy}} of {{Biology}}},
  date      = {2007},
  title     = {From {{Ecological Diversity}} to {{Biodiversity}}},
  doi       = {10.1017/CCOL9780521851282.021},
