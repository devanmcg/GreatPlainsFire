  date         = {2016-04},
  journaltitle = {For Ecol Manage},
  title        = {Prescribed Fire Increases Pyrogenic Carbon in Litter and Surface Soil in Lowland {{Eucalyptus}} Forests of South-Eastern {{Australia}}},
  doi          = {10.1016/j.foreco.2016.01.038},
  issn         = {03781127},
