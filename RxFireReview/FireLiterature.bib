  date         = {2005-08},
  journaltitle = {Soil Sci Plant Nutr},
  title        = {Effects of {{Shifting Cultivation}} on {{Soil Ecosystems}} in {{Sarawak}}, {{Malaysia}}. {{III}}. {{Results}} of {{Burning Practice}} and {{Changes}} in {{Soil Organic Matter}} at {{Niah}} and {{Bakam Experimental Sites}}},
