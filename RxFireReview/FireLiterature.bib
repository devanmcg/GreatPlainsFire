@Article{ash1988,
  author       = {Ash, J.},
  date         = {1988},
  journaltitle = {J Biogeogr},
  title        = {The {{Location}} and {{Stability}} of {{Rainforest Boundaries}} in {{North}}-{{Eastern Queensland}}, {{Australia}}},
  doi          = {10.2307/2845440},
  eprint       = {2845440},
  eprinttype   = {jstor},
  issn         = {0305-0270},
  number       = {4},
  pages        = {619--630},
  volume       = {15},
  abstract     = {The boundaries between rainforest and pyrophytic vegetation (open, sclerophyll and eucalypt forest, and grassland) in north-eastern Queensland, Australia (lat. 17-19⚬ S) are mapped and classified in terms of boundary width and environmental characteristics: mean annual rainfall, substrate and topography. The fertility of different substrates in ranked in relation to the mean annual rainfall required for rainforest: basalt{$>$} alluvium{$>$}sedimentary metamorphics{$>$}granite. The relationship between fire behaviour and hillslope is suggested as an explanation of the slope of rainforest boundaries (boundary slope \$\textbackslash thicksimeq hillslope - 5\textbackslash circ\$). Although rainforest is usually down-slope of pyrophytic vegetation, in some situations rainforest may occur at higher altitudes but it is rarely directly upslope: rainforest occurs in valleys with pyrophytic vegetation on adjacent ridges. Only where the upper substrate is considerably more fertile does rainforest occur directly upslope, and then only on slopes of less than about 206⚬. Changes in boundary location are related to changes in the fire regime, and the long-term migration of rainforest is discussed. Site characteristics are insufficient to predict the location of rainforest boundaries without a knowledge of the vegetation and fire history.},
  publisher    = {{Wiley}},
}

