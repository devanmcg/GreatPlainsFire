@Article{shebitz2009,
  author       = {Shebitz, Daniela Joy and Reichard, Sarah Hayden and Dunwiddie, Peter W.},
  date         = {2009-09-01},
  journaltitle = {Ecol Restor},
  title        = {Ecological and {{Cultural Significance}} of {{Burning Beargrass Habitat}} on the {{Olympic Peninsula}}, {{Washington}}},
