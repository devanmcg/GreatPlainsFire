  author       = {Keane, Robert E. and Hessburg, Paul F. and Landres, Peter B. and Swanson, Fred J.},
  date         = {2009-09},
  journaltitle = {For Ecol Manage},
  title        = {The Use of Historical Range and Variability ({{HRV}}) in Landscape Management},
