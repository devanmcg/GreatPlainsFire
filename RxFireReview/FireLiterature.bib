@Article{bliegebird2018,
  author       = {{Bliege Bird}, Rebecca and Bird, Douglas W. and Fernandez, Luis E. and Taylor, Nyalanka and Taylor, Wakka and Nimmo, Dale},
  date         = {2018-03},
  journaltitle = {Biol Conserv},
  title        = {Aboriginal Burning Promotes Fine-Scale Pyrodiversity and Native Predators in {{Australia}}'s {{Western Desert}}},
