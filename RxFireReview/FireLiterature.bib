@InProceedings{xanthopoulos2008,
  author     = {Xanthopoulos, Gavril},
  booktitle  = {{{II Int Symp Fire Econ Plann Pol}}: {{A Global View}}. \emph{Gen Tech Rep {{PSW-GTR-208}}}},
  date       = {2008},
  title      = {Who Should Be Responsible for Forest Fires? {{Lessons}} from the {{Greek}} Experience.},
