@Article{goforth2005,
  author       = {Goforth, Brett R. and Graham, Robert C. and Hubbert, Kenneth R. and Zanner, C. William and Minnich, Richard A.},
  date         = {2005},
  journaltitle = {Int J Wildland Fire},
  title        = {Spatial Distribution and Properties of Ash and Thermally Altered Soils after High-Severity Forest Fire, Southern {{California}}},
