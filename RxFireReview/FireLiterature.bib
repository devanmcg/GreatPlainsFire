@Article{belillas1998,
  author       = {Belillas, Cm and Feller, Mc},
  date         = {1998},
  journaltitle = {Int J Wildland Fire},
  title        = {Relationships {{Between Fire Severity}} and {{Atmospheric}} and {{Leaching Nutrient Losses}} in {{British Columbia}}'s {{Coastal Western Hemlock Zone Forests}}},
