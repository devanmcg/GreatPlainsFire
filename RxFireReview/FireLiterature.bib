  date         = {1984},
  journaltitle = {J Biogeogr},
  title        = {Geographic Variation in the Demographic Structure of Stands of {{{\emph{Eucalyptus}}}}{\emph{ delegatensis}}{\emph{{\emph{ r. t. Baker on Dolerite in }}}}{{{\emph{{\emph{Tasmania}}}}}}},
  doi          = {10.2307/2844806},
