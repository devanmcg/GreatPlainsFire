@Article{stephens2012,
  author       = {Stephens, Scott L. and McIver, James D. and Boerner, Ralph E. J. and Fettig, Christopher J. and Fontaine, Joseph B. and Hartsough, Bruce R. and Kennedy, Patricia L. and Schwilk, Dylan W.},
  date         = {2012-06-01},
  journaltitle = {Bioscience},
  title        = {The {{Effects}} of {{Forest Fuel}}-{{Reduction Treatments}} in the {{United States}}},
  doi          = {10.1525/bio.2012.62.6.6},
  issn         = {0006-3568},
  number       = {6},
  pages        = {549--560},
  url          = {https://doi.org/10.1525/bio.2012.62.6.6},
  urldate      = {2019-10-10},
  volume       = {62},
  abstract     = {The current conditions of many seasonally dry forests in the western and southern United States, especially those that once experienced low- to moderate-intensity fire regimes, leave them uncharacteristically susceptible to high-severity wildfire. Both prescribed fire and its mechanical surrogates are generally successful in meeting short-term fuel-reduction objectives such that treated stands are more resilient to high-intensity wildfire. Most available evidence suggests that these objectives are typically accomplished with few unintended consequences, since most ecosystem components (vegetation, soils, wildlife, bark beetles, carbon sequestration) exhibit very subtle effects or no measurable effects at all. Although mechanical treatments do not serve as complete surrogates for fire, their application can help mitigate costs and liability in some areas. Desired treatment effects on fire hazards are transient, which indicates that after fuel-reduction management starts, managers need to be persistent with repeated treatment, especially in the faster-growing forests in the southern United States.},
  shortjournal = {BioScience},
}

