@Article{takaoka1996,
  author       = {Takaoka, Sadao and Sasa, Kaichiro},
  date         = {1996-12-01},
  journaltitle = {Ecol Res},
  title        = {Landform Effects on Fire Behavior and Post-Fire Regeneration in the Mixed Forests of Northern {{Japan}}},
