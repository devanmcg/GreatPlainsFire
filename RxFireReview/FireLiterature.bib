@Article{olaughlin2005,
  author       = {O’Laughlin, Jay},
  date         = {2005-06-06},
  journaltitle = {For Ecol Manage},
  title        = {Policy Issues Relevant to Risk Assessments, Balancing Risks, and the {{National Fire Plan}}: {{Needs}} and Opportunities},
