@article{mcclain2021,
  title = {Patterns of {{Anthropogenic Fire}} within the {{Midwestern Tallgrass Prairie}} 1673--1905: {{Evidence}} from {{Written Accounts}}},
  shorttitle = {Patterns of {{Anthropogenic Fire}} within the {{Midwestern Tallgrass Prairie}} 1673--1905},
  author = {McClain, William E. and Ruffner, Charles M. and Ebinger, John E. and Spyreas, Greg},
  year = {2021},
