@article{ren2024,
  title = {Earlier Spring Greening in {{Northern Hemisphere}} Terrestrial Biomes Enhanced Net Ecosystem Productivity in Summer},
  author = {Ren, Yijia and Qiu, Jianxiu and Zeng, Zhenzhong and Liu, Xiaoping and Sitch, Stephen and Pilegaard, Kim and Yang, Tianyao and Wang, Sheng and Yuan, Wenping and Jain, Atul K.},
