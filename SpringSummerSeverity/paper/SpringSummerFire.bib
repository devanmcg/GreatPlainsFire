@article{decker2019,
  title = {Growing or Dormant Season Burns: The Effects of Burn Season on Bee and Plant Communities},
  shorttitle = {Growing or Dormant Season Burns},
  author = {Decker, Brenna L. and {Harmon-Threatt}, Alexandra N.},
