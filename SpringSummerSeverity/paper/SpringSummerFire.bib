@article{escuin2008,
  title = {Fire Severity Assessment by Using {{NBR}} ({{Normalized Burn Ratio}}) and {{NDVI}} ({{Normalized Difference Vegetation Index}}) Derived from {{LANDSAT TM}}/{{ETM}} Images},
